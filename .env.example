APP_NAME=ConnectV2
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://connect-v2.localhost
APP_PORT=80

# Docker specific settings
COMPOSE_PROJECT_NAME=connect-v2
DOCKER_PHP_VERSION=8.3
DOCKER_NGINX_PORT=80
DOCKER_PHP_PORT=9000
DOCKER_MYSQL_PORT=3306
DOCKER_REDIS_PORT=6379
DOCKER_MAILHOG_PORT=1025
DOCKER_MAILHOG_DASHBOARD_PORT=8025

# Database settings
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=connect_v2
DB_USERNAME=connect_user
DB_PASSWORD=secret_password
DB_ROOT_PASSWORD=root_secret

# Redis settings
REDIS_HOST=redis
REDIS_PASSWORD=redis_secret
REDIS_PORT=6379

# Mail settings for development (MailHog)
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Session and Cache
SESSION_DRIVER=redis
CACHE_DRIVER=redis
QUEUE_CONNECTION=redis
SESSION_LIFETIME=120

# Other Laravel settings
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug
BROADCAST_DRIVER=log
FILESYSTEM_DISK=local
MEMCACHED_HOST=127.0.0.1

# Vite configuration
VITE_APP_NAME="${APP_NAME}"